{
	"author": "Azamat Mambetov @MrMurdock11",
	"bin": {
		"noty": "./bin/run.js"
	},
	"dependencies": {
		"@iarna/toml": "^2.2.5",
		"@inquirer/select": "^2.1.0",
		"@oclif/core": "^3",
		"@oclif/plugin-help": "^6",
		"@oclif/plugin-plugins": "^4"
	},
	"description": "oclif example Hello World CLI",
	"devDependencies": {
		"@oclif/prettier-config": "^0.2.1",
		"@oclif/test": "^3",
		"@types/chai": "^4",
		"@types/mocha": "^9.0.0",
		"@types/node": "^18",
		"chai": "^4",
		"eslint": "^8.56.0",
		"eslint-config-oclif": "^5",
		"eslint-config-oclif-typescript": "^3",
		"eslint-config-prettier": "^9.1.0",
		"mocha": "^10",
		"oclif": "^4",
		"shx": "^0.3.3",
		"ts-node": "^10.9.2",
		"typescript": "^5"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"files": [
		"/bin",
		"/dist",
		"/oclif.manifest.json"
	],
	"homepage": "https://github.com/MrMurdock11/noty",
	"license": "MIT",
	"main": "dist/index.js",
	"name": "noty",
	"oclif": {
		"bin": "noty",
		"dirname": "noty",
		"commands": "./dist/commands",
		"plugins": [
			"@oclif/plugin-help",
			"@oclif/plugin-plugins"
		],
		"hooks": {
			"init": "./dist/hooks/init/init-storage"
		}
	},
	"repository": "MrMurdock11/noty",
	"scripts": {
		"build": "shx rm -rf dist && tsc -b",
		"lint": "eslint . --ext .ts",
		"postpack": "shx rm -f oclif.manifest.json",
		"posttest": "npm run lint",
		"prepack": "npm run build && oclif manifest && oclif readme",
		"prepare": "npm run build",
		"test": "mocha --forbid-only \"test/**/*.test.ts\"",
		"version": "oclif readme && git add README.md"
	},
	"version": "0.1.0",
	"bugs": "https://github.com/MrMurdock11/noty/issues",
	"keywords": [
		"oclif"
	],
	"types": "dist/index.d.ts"
}
